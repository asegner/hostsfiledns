<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>GroupName</key>
    <string>wheel</string>
    <key>InitGroups</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>Label</key>
    <string>net.segner.util.hostsfiledns</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/hostsfiledns</string>
        <string>-u</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>ThrottleInterval</key>
    <integer>10800</integer>
    <key>UserName</key>
    <string>root</string>
</dict>
</plist>
